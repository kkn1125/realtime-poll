<table>
  <thead>
    <tr>
      <th>
        <div>
          <img
            src={{image}}
            alt='snappoll-logo'
            width='35'
            height='35'
          />{{title}}
        </div>
      </th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td>
        <div>
          <h2>
            SnapPoll 안내
          </h2>
          <div class='alert'>
            {{content}}
          </div>
        </div>
      </td>
    </tr>
    <tr>
      <td>
        <button onclick='window.close()'>닫기</button>
      </td>
    </tr>
  </tbody>
  <tfoot>
    <tr>
      <td>
        <a href='mailto:{{email}}'>{{email}}</a>
      </td>
    </tr>
  </tfoot>
</table>